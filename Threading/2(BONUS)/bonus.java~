class thone extends Thread {
  int maxcount=1;
  int number=1;
  int i;
    public void run() {       
      for ( i = 2;  i <= 10000;  i++ ) {      
           int count=0;            
           for ( int j = 1;  j <= i;  j++ ) { 
              if ( i % j == 0 )
                 count++;
           }           
           if (count > maxcount) {
              maxcount = count;
              number = i;
           }       
       }
    }
   public int divisor1() {
      int div=maxcount;
      return div;
    }
   public int number1() {
      int num=number;
      return num;
    }
    }
class thtwo extends Thread {
  int maxcount=1;
  int number=1;
  int i;
    public void run() {       
      for ( i = 10001;  i <= 20000;  i++ ) {      
           int count=0;            
           for ( int j = 1;  j <= i;  j++ ) { 
              if ( i % j == 0 )
                 count++;
           }           
           if (count > maxcount) {
              maxcount = count;
              number = i;
           }       
       }
    }
   public int divisor2() {
      int div=maxcount;
      return div;
    }
   public int number2() {
      int num=number;
      return num;
    }
    }
